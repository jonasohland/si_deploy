{
  "$ref": "#/definitions/UserData",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "ArtistSyncSettings": {
      "additionalProperties": false,
      "properties": {
        "settings": {
          "additionalProperties": false,
          "properties": {
            "first_device_channel": {
              "type": "number"
            },
            "first_port": {
              "type": "number"
            },
            "last_port": {
              "type": "number"
            },
            "node": {
              "type": "number"
            }
          },
          "required": [
            "node",
            "first_port",
            "last_port",
            "first_device_channel"
          ],
          "type": "object"
        },
        "user_panel": {
          "$ref": "#/definitions/Port"
        }
      },
      "required": [
        "settings",
        "user_panel"
      ],
      "type": "object"
    },
    "Port": {
      "additionalProperties": false,
      "properties": {
        "IsInput": {
          "type": "boolean"
        },
        "Node": {
          "type": "number"
        },
        "Port": {
          "type": "number"
        }
      },
      "required": [
        "Node",
        "Port",
        "IsInput"
      ],
      "type": "object"
    },
    "UserData": {
      "additionalProperties": false,
      "properties": {
        "artist": {
          "$ref": "#/definitions/ArtistSyncSettings"
        },
        "channel": {
          "type": "number"
        },
        "headtracker": {
          "type": "number"
        },
        "id": {
          "type": "string"
        },
        "inputs": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "name": {
          "type": "string"
        },
        "room": {
          "type": "string"
        },
        "xtc": {
          "$ref": "#/definitions/XTCSettings"
        }
      },
      "required": [
        "name",
        "id",
        "channel",
        "headtracker",
        "xtc",
        "artist",
        "inputs"
      ],
      "type": "object"
    },
    "XTCSettings": {
      "additionalProperties": false,
      "properties": {
        "accuracy": {
          "type": "number"
        },
        "dist_ears": {
          "type": "number"
        },
        "dist_listener": {
          "type": "number"
        },
        "dist_spk": {
          "type": "number"
        },
        "enabled_bin": {
          "type": "boolean"
        },
        "enabled_st": {
          "type": "boolean"
        }
      },
      "required": [
        "accuracy",
        "enabled_st",
        "enabled_bin",
        "dist_spk",
        "dist_ears",
        "dist_listener"
      ],
      "type": "object"
    }
  }
}